{
    "compilerOptions": {
      "target": "ES2022",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true,
      "allowImportingTsExtensions": true
    },
    "typedocOptions": {
      "entryPoints": ["src/models/*.ts"],
      "out":"./docs/typedoc",
      "exclude": ["**/node_modules/**"]

    },
    "include": ["src", "docs"]
  }
  